option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: popol.wsgi:application
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: popol.settings
  aws:elasticbeanstalk:environment:proxy:staticfiles:
    /static: static
  aws:elasticbeanstalk:s3:uploadconfiguration:
    Bucket: ebpopol-s3
    UploadTimeout: '3600'